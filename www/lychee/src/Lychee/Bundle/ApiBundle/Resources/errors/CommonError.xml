<?xml version="1.0" ?>

<section code="1">
    <group>
        <error name="UnknownError">{message}</error>
        <error name="SystemError"/>
        <error name="SystemBusy"/>
        <error name="ServiceUnavailable"/>
        <error name="PermissionDenied"/>
        <error name="TooManyRequest" display="请求太多，请稍后再试"/>
        <error name="SignatureInvalid" display="签名验证失败">Signature invalid</error>
    </group>
    <group>
        <error name="RequestIllegal"/>
        <error name="RequestInvalid"/>
        <error name="ApiNotFound">can not find a api match '{uri}'</error>
        <error name="MethodNotAllow">method {method} not allow, and it allows '{allows}'</error>
        <error name="ParameterInvalid">parameter '{parameter}' with '{value}' invalid</error>
        <error name="ParameterMissing">missing parameter '{parameter}'</error>
        <error name="ObjectNonExist">object with id {objectId} no exist</error>
        <error name="PleaseUseUTF8">some parameter contains non utf8 chars</error>
        <error name="DeviceBlocked" display="设备因违规已被封印，请联系次元酱！"/>
        <error name="ContainsReservedWords" display="官方字已被官方占领~" />
    </group>
</section>