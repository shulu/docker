<body>
<section id="userpanel">
    <div id="usericon" class="j"><img src="{{ userData.avatar_url }}"/></div>
    <div id="usermainpanel">
        <div id="usernickname" class="j">{{ userData.nickname|escape|trim }}
            {% if userData.level <= 2 %}
                {% set levelClass = 'cloud-gray' %}
            {% elseif userData.level > 2 and userData.level <= 5 %}
                {% set levelClass = 'star-gray' %}
            {% elseif userData.level > 5 and userData.level <= 8 %}
                {% set levelClass = 'star-silver' %}
            {% elseif userData.level > 8 and userData.level <= 12 %}
                {% set levelClass = 'moon-gray' %}
            {% elseif userData.level > 12 and userData.level <= 16 %}
                {% set levelClass = 'moon-silver' %}
            {% elseif userData.level > 16 and userData.level <= 20 %}
                {% set levelClass = 'moon-golden' %}
            {% elseif userData.level > 20 and userData.level <= 25 %}
                {% set levelClass = 'sun-gray' %}
            {% elseif userData.level > 25 and userData.level <= 30 %}
                {% set levelClass = 'sun-silver' %}
            {% elseif userData.level > 30 and userData.level <= 35 %}
                {% set levelClass = 'sun-golden' %}
            {% elseif userData.level > 35 %}
                {% set levelClass = 'sun-purple' %}
            {% endif %}
            <div class="level {{ levelClass }} j">{{ userData.level }}</div>
        </div>
        <div id="userinfo2">{% if userData.gender == 'male' %}<img  class="j" src="http://qn.ciyocon.com//icons/boy.2x.png"/>{% elseif userData.gender == 'female' %}<img src="http://qn.ciyocon.com//icons/girl.2x.png"  class="j"/>{% endif %} {{ userData.location|escape|trim }}{% if userData.location != '' and userData.age != null %}·{% endif %}{% if userData.age != null %}{{ userData.age|escape|trim }}岁{% endif %}</div>
        <div id="usersignature" class="j">{{ userData.signature|escape|trim }}</div>
        <div id="userfollow">
            <span  class="j">
                <strong>{{ userData.followers_count|default(0) }}</strong> <strong><span>粉丝</span></strong> <strong>{{ userData.followees_count|default(0) }}</strong> <strong><span>关注</span></strong>
            </span>
        </div>
    </div>
    <div id="nav">
        <section class="current">帖子</section>
        <section>视频</section>
    </div>
</section>
<section id="text" class="contentlist cur">
    <section class="content">
        {% for post in posts %}
        <section class="post_panel">
            <div class="post_time">{{ timeStr(post.create_time) }}</div>
            <div class="post_content j">{{ post.content | nl2br }}
                {% if annotation.multi_photos is defined and annotation.multi_photos is not empty %}
                    {% set images = annotation.multi_photos %}
                {% elseif post.image_url is defined and post.image_url is not empty %}
                    {% set images = [post.image_url] %}
                {% else %}
                    {% set images = [] %}
                {% endif %}

                {% set imgWidth = images|length > 1 ? 200 : 640 %}
                {% set imgHeight = images|length > 1 ? 200 : 0 %}
                {% set tmpImages = [] %}
                {% for img in images %}
                    {% set tmpImages = tmpImages|merge([{url: img|resizeImage(imgWidth, imgHeight)}]) %}
                {% endfor %}
                {% set images = tmpImages %}

                {% if annotation.multi_photo_gif_index is defined and annotation.multi_photo_gif_index is not empty %}
                    {% set tmpImgs = [] %}
                    {% for i in annotation.multi_photo_gif_index|default([]) %}
                        {% if images[i] is defined %}
                            {% set tmpImgs = tmpImgs|merge([{url: images[i].url, gif: true}]) %}
                        {% else %}
                            {% set tmpImgs = tmpImgs|merge(images[i]) %}
                        {% endif %}
                    {% endfor %}
                    {% set images = tmpImgs %}
                {% endif %}

                {% if annotation.multi_photo_widths is defined and annotation.multi_photo_heights is defined and images|length == annotation.multi_photo_widths|length and annotation.multi_photo_widths|length == annotation.multi_photo_heights|length %}
                    {% set tmpImgs = [] %}
                    {% for w in annotation.multi_photo_widths %}
                        {% set i = loop.index0 %}
                        {% set h = annotation.multi_photo_heights[loop.index0]|default(0) %}
                        {% if images[i] is defined and w > 0 and (h / w >= 1.5) %}
                            {% set image = images[i]|merge({long: true}) %}
                            {% set tmpImgs = tmpImgs|merge([image]) %}
                        {% else %}
                            {% set tmpImgs = tmpImgs|merge([images[i]]) %}
                        {% endif %}
                    {% endfor %}
                    {% set images = tmpImgs %}
                {% endif %}

                {% if images is not empty %}
                    <div class="posts_image j">
                        {% for image in images %}
                            <img src="{{ image.url }}/>
                        {% endfor %}
                    </div>
                {% endif %}

            </div>
            <span class="post_ciyo j">{{ post.topic.title }}</span>
            <div class="post_action">
                <span class="post_like j">{{ post.liked_count }}</span>
                <span class="post_comment j">{{ post.commented_count }}</span>
                <span class="post_share j"></span>
            </div>
        </section>
        {% else %}
            <div class="empty">TA还没有发布帖子哦</div>
        {% endfor %}
    </section>
</section>
<section id="video" class="contentlist">
    <section class="content">
        {% for video in videos %}
                <section class="video j">
                    <img src="{{ video.image_url }}/>
                    <span>{{ video.liked_count }}</span>
                </section>
        {% else %}
            <div class="empty">TA还没有发布视频哦</div>
        {% endfor %}
    </section>
</section>
<footer>
    <img class="j" src="http://qn.ciyocon.com/app/icons/v2/logo.png?v2"/>
    <button class="j">打开看看</button>
</footer>
</body>

<script type="text/javascript">
    $(document).ready(function(){
        $('#nav > section').click(function(){
            if($(this).hasClass('current')) return false;
            $('#nav > section').toggleClass('current');
            $('.contentlist').toggle();
        });
        $('.j').click(function(){
            var url='http://sj.qq.com/myapp/detail.htm?apkName=com.infothinker.erciyuan';
            window.location.href=url;
            return false;
        })
    });
</script>
</html>
